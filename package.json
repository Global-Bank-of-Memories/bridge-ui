    "@angular/router": "~13.3.1",
    "@auth0/angular-jwt": "^5.0.2",
    "@ng-bootstrap/ng-bootstrap": "^12.1.2",
    "@popperjs/core": "^2.10.2",
    "bootstrap": "^4.6.1",
    "express": "^4.17.3",
